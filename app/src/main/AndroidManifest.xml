<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.easybursaries">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EasyBursaries"
        tools:targetApi="31">
        <activity
            android:name=".AboutUs"
            android:exported="false"
            android:label="About Us"/>
        <activity
            android:name=".Privacy_Policy"
            android:exported="false"
            android:label="Privacy Policy" />
        <activity
            android:name=".TellAFriend"
            android:exported="false"
            android:label="Tell A friend" />
        <activity
            android:name=".SettingsPage"
            android:exported="false"
            android:label="Settings" />
        <activity
            android:name=".SignUpPage"
            android:exported="false" />
        <activity
            android:name=".LoginPage"
            android:exported="false" />
        <activity
            android:name=".HomePage"
            android:exported="false" />
        <activity
            android:name=".ProfileDesignPage"
            android:exported="false"
            android:label="@string/Profile_heading" />
        <activity
            android:name=".NotificationsPage"
            android:exported="false"
            android:label="@string/title_bar" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>